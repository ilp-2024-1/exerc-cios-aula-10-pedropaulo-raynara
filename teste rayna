print ('número primo')
n = int(input("digite um número entre 1 e 10.000:"))
i=2

if n <=1: False
else:
    if n == 2: True
    elif n % 2 == 0: False
    else: 
        i = 3
    while i * i <= n:
        if n % i ==0: False
        i += 2

if n:
    (print (f'o número {n} é primo'))
else:
    (print (f'o numero {n} não é primo'))  